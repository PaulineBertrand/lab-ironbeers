<div class="random-beer-container">
    {{> beerpartial randomBeer}}
    <div class="food-pairing">
        <h4>Food pairing</h4>
        <ul>
            {{#each randomBeer.food_pairing}}
                <li>{{this}}</li>
            {{/each}}
        </ul>
    </div>
    <div class="brewer-tips">
        <h4>Brewer tips</h4>
        <p>{{randomBeer.brewers_tips}}</p>
    </div> 
</div>